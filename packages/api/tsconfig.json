{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "downlevelIteration": true
  },
  "include": ["src/"]
}
